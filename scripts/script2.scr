
for (i = 0; i < 30; i++) {
    if (i % 3 == 0 && i % 5 == 0)
        output('Fizz Buzz');
    else if (i % 3 == 0)
        output('Fizz');
    else if (i % 5 == 0)
        output('Buzz');
    else
        output(i);
}

function fibonacci(i) {
    if (i <= 2) return i;
    breakpoint;
    return (fibonacci(i - 1) + fibonacci(i - 2));
}

for (i = 0; i < 11; i++) {
    output("Fibonacci of", i, "is", fibonacci(i));
}

return 0;
